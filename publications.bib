	eprint = {1707.00308},
	primaryClass = "math.PR",
	keywords = {Delaunay, geometry, Mathematics - Probability, Mathematics - Geometric Topology, 60G55, 22E40, 43A07}
}

@ARTICLE{LambertPaquette01,
